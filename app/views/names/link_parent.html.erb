<h1><%= @name.name_html %></h1>
<hr/>

<%= simple_form_for(@name, url: name_link_parent_commit_path(@name), html: { method: :post }) do |f| %>
  <%= f.input :parent, required: true, autofocus: true,
        input_html: { value: @name.parent&.name } %>
  <%= f.button :submit, 'Submit' %>
<% end %>


